    "staging": "pm2-runtime start ecosystem.config.js --env staging",
    "typeorm": "ts-node -r tsconfig-paths/register --transpile-only --project tsconfig.json ./node_modules/typeorm/cli.js",
    "lint": "eslint src -c .eslintrc.js --ext \".js,.ts\" --cache",
    "prettify": "prettier --check 'src/**/*.{ts,js,json}' --write",
    "test": "jest --runInBand",
    "test:changed": "jest -o --passWithNoTests",
    "test:ci": "TZ=America/Fortaleza jest --ci --runInBand --collectCoverage"
